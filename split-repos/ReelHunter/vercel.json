{
  "name": "reelhunter",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_DOMAIN": "reelhunter.reelapps.co.za",
    "VITE_AUTH_DOMAIN": "reelapps.co.za"
  }
} 